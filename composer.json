{
  "name": "byjg/wordnumber",
  "description": "Convert numeric values into their word representations in multiple languages.",
  "require": {
    "php": ">=8.3 <8.6"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\WordNumber\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^11.5",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "dev"
}
